.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  text-align: center;
  padding: 19px 50px;
  min-height: 103px;
  border-bottom: 1px solid #5f5959;
}

.content {
  text-align: center;
  height: calc(100vh - 103px);
  padding-inline: 107px;
  padding-block: 69px;
  overflow: auto;
}

.title {
  left: 50%;
  transform: translateX(-50%);
  color: #fff;
  text-transform: uppercase;
  margin: 0;
  position: absolute;
}
