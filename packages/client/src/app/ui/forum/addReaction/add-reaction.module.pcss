.button {
  min-width: 32px;
  height: 32px;
  padding: 7px;
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 6px;
  border-radius: 4px;
  font-size: 14px;
  font-weight: 500;
}

.button-icon {
  color: #aaa;
}

.button-count {
  color: black;
  font-size: 14px;
  line-height: 16px;
}
