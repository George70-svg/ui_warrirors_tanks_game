.video-background {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;

  video {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }
}

.container {
  position: relative;
  z-index: 2;
  height: 100%;

  .image {
    max-width: 50%;
  }
}
