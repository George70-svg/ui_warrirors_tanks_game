.sign-in-page {
  width: 100dvw;
  height: 100dvh;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #b1b1b1;
}

.sign-in-container {
  width: 440px;
  height: 575px;
  border-radius: 16px;
  background-color: #0d0f1c;
  padding: 2rem;
}

.to-sign-up-container {
  display: flex;
  justify-content: center;
  gap: 0.5rem;
}

.ant-form-item-explain-error {
  font-size: 14px;
}
